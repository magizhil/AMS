@using AMS.Models
@model AMS.Models.DBMultiview

@{
    ViewBag.Title = "DashboardSoft";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@*<script src="~/Scripts/jquery-1.8.0.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>*@

<div class="w3-container">
    <div class="w3-row">
        <div class="w3-border">
            <div class="w3-container">

                <table class="w3-table w3-striped w3-border">
                    <tr class="w3-blue">
                        <th>
                            Category Name
                        </th>
                        <th>
                            Model
                        </th>
                        <th>
                            User License
                        </th>
                        <th>
                            Machine License
                        </th>
                    </tr>

                    @foreach (DBSwAstDet swdet in Model.dbswastdet)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => swdet.CATGNAME)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => swdet.MDLNAME)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => swdet.USERLIC)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => swdet.MACLIC)
                            </td>
                        </tr>
                    }

                </table>


            </div>
        </div>
    </div>
</div>
